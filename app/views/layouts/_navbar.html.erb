<%# test update gitHub %>
 <nav class="navbar navbar-expand-lg navbar-light bg-light">
 <div class="container">
      <%= link_to 'RubyBlog', root_path, class: 'navbar-brand' %> 
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="navbarNav">
        <ul class="navbar-nav">
          <li class="nav-item">
            <%= link_to "Home", root_path, class: 'nav-link' %>
          </li>
          <li class="nav-item">
              <%= link_to 'Articles', articles_url, class: 'nav-link' %> 
          </li>
          <% if current_user && current_user.has_role?(:editor) %>
            <li class="nav-item">
              <%= link_to 'Product', products_path, class: 'nav-link' %>
            </li>
          <% else %>
            <li class="nav-item">
                <%= link_to 'Shops', shops_path, class: 'nav-link' %> 
            </li>  
          <% end %>
          <li class="nav-item">
            <%= link_to 'About', about_index_url, class: 'nav-link' %>
          </li>
        </ul>
      </div>
      <div class="navbar-collapse collapse w-100 order-3 dual-collapse2 " id="navbarNav">
        <ul class="navbar-nav ml-auto">
            <li class="nav-item">
              <%= link_to " Cart #{current_order.order_items.size} ", carts_path, class: 'nav-link' %>
            </li>
          <% if !user_signed_in? %>
            <li class="nav-item">
              <%= link_to 'Login', new_user_session_path, class: 'nav-link' %>
            </li>
          <% else %>
            <li class="nav-item">
             <%= link_to 'Sign out', destroy_user_session_path, :method => :delete, class: 'nav-link' %>
            </li>
            <li class="nav-item">
             <%= link_to 'Profile', edit_user_registration_path, class: 'nav-link' %>
            </li>
            <li class="nav-item">
              <div class="nav-link disabled"><%= current_user.fullname %></div> 
            </li>
          <% end %>
        </ul>
    </div>
  </div>
</nav>